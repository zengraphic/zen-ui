{
  "name": "zen-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "watch": "ng build --watch --configuration development",
    "test": "ng test zen-uikit-lib",
    "watch:sass": "sass --watch projects/zen-uikit-lib/src/styles/zen-uikit-lib.scss:projects/zen-uikit-lib/src/styles/zen-uikit-lib.min.css --style=compressed",
    "sass": "sass projects/zen-uikit-lib/src/styles/zen-uikit-lib.scss:projects/zen-uikit-lib/src/styles/zen-uikit-lib.min.css --style=compressed",
    "build": "yarn sass && node ./scripts/build-libraries.js build --lib zen-uikit-lib",
    "start": "node ./scripts/build-libraries.js start --lib zen-uikit-lib",
    "start:tester": "ng serve lib-tester",
    "doc:generate": "npx compodoc -p tsconfig.json projects/zen-uikit-lib/src --hideGenerator",
    "doc:serve": "lite-server",
    "doc": "yarn doc:generate",
    "doc:pages": "./node_modules/.bin/compodoc -p tsconfig.json projects/zen-uikit-lib/src",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "yarn docs:json && start-storybook -p 6006",
    "build-storybook": "yarn docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.2.5",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@ng-select/ng-select": "^9.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "cleave.js": "^1.6.0",
    "core-js": "^3.25.5",
    "node-sass": "^7.0.3",
    "reset-css": "^5.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.7",
    "@angular/cli": "~14.0.1",
    "@angular/compiler-cli": "^14.0.0",
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-a11y": "^6.5.14",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/angular": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/angular": "^13.4.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "18.7.23",
    "babel-loader": "^8.3.0",
    "cpx": "^1.5.0",
    "css-loader": "^6.7.2",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lite-server": "^2.6.1",
    "ng-packagr": "^14.0.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "scss-bundle": "^3.1.2",
    "semver": "^7.3.8",
    "shelljs": "^0.8.5",
    "style-loader": "^3.3.1",
    "typescript": "~4.7.2",
    "yargs": "^17.6.0"
  }
}